{
  "name": "ai-chat",
  "version": "1.0.0",
  "description": "Static AI Chat UI with ESLint, Prettier and Husky",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "lint": "eslint .",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^10.0.2",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "typescript-eslint": "^8.56.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "prettier --check"
    ],
    "*.{html,css}": [
      "prettier --check"
    ]
  }
}
